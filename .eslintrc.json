{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "prefer-const": "warn",
    "no-use-before-define": "warn",

    // Airbnb base rules
    "import/extensions": ["error", "ignorePackages"],
    "import/prefer-default-export": "off"

    // Prettier rules
    // "prettier/prettier": "error" use only when eslint-plugin-prettier in use
  }
}
